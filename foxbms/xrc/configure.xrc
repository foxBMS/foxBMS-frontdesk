<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><resource xmlns="http://www.wxwindows.org/wxxrc" version="2.3.0.1">	<object class="wxPanel" name="configure_panel">		<style>wxTAB_TRAVERSAL</style>		<size>500,300</size>		<object class="wxBoxSizer">			<orient>wxVERTICAL</orient>			<object class="sizeritem">				<option>0</option>				<flag>wxEXPAND</flag>				<border>5</border>				<object class="wxBoxSizer">					<orient>wxHORIZONTAL</orient>					<object class="sizeritem">						<option>0</option>						<flag>wxALL</flag>						<border>5</border>						<object class="wxButton" name="generate_b">							<label>Write Configuration to Files</label>							<default>0</default>						</object>					</object>					<object class="sizeritem">						<option>0</option>						<flag>wxALL</flag>						<border>5</border>						<object class="wxButton" name="reset_b">							<label>Reset all values to default</label>							<default>0</default>						</object>					</object>					<object class="sizeritem">						<option>0</option>						<flag>wxALL</flag>						<border>5</border>						<object class="wxButton" name="load_b">							<label>Load Configuration</label>							<default>0</default>						</object>					</object>					<object class="sizeritem">						<option>0</option>						<flag>wxALL</flag>						<border>5</border>						<object class="wxButton" name="save_b">							<label>Save Configuration</label>							<default>0</default>						</object>					</object>				</object>			</object>			<object class="sizeritem">				<option>1</option>				<flag>wxALL|wxEXPAND</flag>				<border>5</border>				<object class="unknown" name="property_grid" />			</object>			<object class="sizeritem">				<option>0</option>				<flag>wxALL|wxEXPAND</flag>				<border>5</border>				<object class="wxHtmlWindow" name="help_txt">					<style>wxHW_SCROLLBAR_AUTO</style>					<size>-1,150</size>				</object>			</object>		</object>	</object></resource>