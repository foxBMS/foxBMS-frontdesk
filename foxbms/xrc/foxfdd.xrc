        <object class="sizeritem">
          <object class="wxStaticText">
            <label>GIT repository</label>
          </object>
          <option>0</option>
          <flag>wxALL</flag>
          <border>5</border>
        </object>
        <object class="sizeritem">
          <object class="wxTextCtrl" name="add_dir_dp"/>
          <option>0</option>
          <flag>wxALL|wxEXPAND</flag>
          <border>5</border>
          <minsize>300,-1</minsize>
        </object>
        <object class="sizeritem">
          <object class="wxBoxSizer">
            <object class="sizeritem">
