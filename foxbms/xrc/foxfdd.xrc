          </object>
          <option>0</option>
          <flag>wxALL|wxEXPAND</flag>
          <border>5</border>
          <minsize>300,-1</minsize>
        </object>
        <object class="sizeritem">
          <object class="wxBoxSizer">
            <object class="sizeritem">
              <object class="wxStaticText">
                <label>Project Name:</label>
              </object>
              <option>0</option>
              <flag>wxALL</flag>
              <border>5</border>
            </object>
            <object class="sizeritem">
              <object class="wxTextCtrl" name="project_name_tc"/>
              <option>1</option>
              <flag>wxALL|wxEXPAND</flag>
              <border>5</border>
            </object>
            <orient>wxHORIZONTAL</orient>
          </object>
          <option>0</option>
          <flag>wxALL|wxEXPAND</flag>
          <border>5</border>
        </object>
        <object class="sizeritem">
          <object class="wxStaticText" name="add_dir_error_txt">
            <size>-1,35</size>
            <wrap>2</wrap>
            <fg>#D70015</fg>
          </object>
          <option>0</option>
          <flag>wxALL|wxEXPAND</flag>
          <border>10</border>
        </object>
        <object class="sizeritem">
          <object class="wxBoxSizer">
            <object class="sizeritem">
              <object class="wxButton" name="wxID_OK">
                <label>Add project</label>
              </object>
              <option>0</option>
              <flag>wxALL</flag>
              <border>5</border>
            </object>
            <object class="sizeritem">
              <object class="wxButton" name="wxID_CANCEL"/>
              <option>0</option>
              <flag>wxALL</flag>
              <border>5</border>
            </object>
            <orient>wxHORIZONTAL</orient>
          </object>
          <option>0</option>
          <flag>wxALL|wxEXPAND</flag>
          <border>5</border>
        </object>
      </object>
    </object>
    <size>300,-1</size>
    <title>Add project from archive</title>
  </object>
