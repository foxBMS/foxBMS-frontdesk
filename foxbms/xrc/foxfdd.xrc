              <flag>wxALL|wxEXPAND</flag>
              <border>5</border>
            </object>
            <orient>wxHORIZONTAL</orient>
          </object>
          <option>0</option>
          <flag>wxALL|wxEXPAND</flag>
          <border>5</border>
        </object>
        <object class="sizeritem">
          <object class="wxStaticText" name="add_dir_error_txt">
            <size>-1,35</size>
