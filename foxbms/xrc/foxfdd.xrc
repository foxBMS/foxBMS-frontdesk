<?xml version="1.0" encoding="UTF-8" standalone="yes" ?><resource xmlns="http://www.wxwindows.org/wxxrc" version="2.3.0.1">	<object class="wxFrame" name="frontdesk_mframe">		<style>wxDEFAULT_FRAME_STYLE|wxTAB_TRAVERSAL</style>		<size>680,715</size>		<title>foxBMS frontdesk</title>		<centered>1</centered>		<aui_managed>0</aui_managed>		<object class="wxBoxSizer">			<orient>wxVERTICAL</orient>			<object class="sizeritem">				<option>1</option>				<flag>wxEXPAND | wxALL</flag>				<border>5</border>				<object class="wxNotebook" name="main_nb">					<object class="notebookpage">						<label>Install foxBMS source project</label>						<selected>0</selected>						<object class="wxPanel" name="install_page">							<style>wxTAB_TRAVERSAL</style>							<object class="wxBoxSizer">								<orient>wxVERTICAL</orient>								<object class="sizeritem">									<option>0</option>									<flag>wxALL</flag>									<border>5</border>									<object class="wxRadioBox" name="project_radio">										<style>wxRA_SPECIFY_COLS</style>										<label>Project</label>										<selection>0</selection>										<content>											<item>Create New Project</item>											<item>Open Existing Project</item>										</content>										<dimension>1</dimension>									</object>								</object>								<object class="spacer">									<option>1</option>									<flag>wxEXPAND</flag>									<border>5</border>									<size>0,0</size>								</object>								<object class="sizeritem">									<option>1</option>									<flag>wxEXPAND | wxALL</flag>									<border>5</border>									<object class="wxPanel" name="new_panel">										<style>wxTAB_TRAVERSAL</style>										<enabled>0</enabled>										<object class="wxBoxSizer">											<orient>wxVERTICAL</orient>											<object class="sizeritem">												<option>0</option>												<flag>wxALL</flag>												<border>5</border>												<object class="wxStaticText" name="archive_st">													<label>Install foxBMS source archive</label>													<wrap>-1</wrap>												</object>											</object>											<object class="sizeritem">												<option>0</option>												<flag>wxALL|wxEXPAND</flag>												<border>5</border>												<object class="wxFilePickerCtrl" name="archive_fp">													<value></value>													<message>Select the foxBMS source archive</message>													<wildcard>*.*</wildcard>													<style>wxFLP_DEFAULT_STYLE</style>												</object>											</object>											<object class="spacer">												<option>1</option>												<flag></flag>												<border>2</border>												<size>0,0</size>											</object>											<object class="sizeritem">												<option>0</option>												<flag>wxALL</flag>												<border>5</border>												<object class="wxStaticText" name="dest_st">													<label>into workspace directory</label>													<wrap>-1</wrap>												</object>											</object>											<object class="sizeritem">												<option>0</option>												<flag>wxALL|wxEXPAND</flag>												<border>5</border>												<object class="wxDirPickerCtrl" name="dest_dp">													<value></value>													<message>Select the workspace folder for the foxBMS project</message>													<style>wxDIRP_DEFAULT_STYLE</style>												</object>											</object>										</object>									</object>								</object>								<object class="spacer">									<option>1</option>									<flag>wxEXPAND</flag>									<border>5</border>									<size>0,0</size>								</object>								<object class="sizeritem">									<option>0</option>									<flag>wxALL</flag>									<border>5</border>									<object class="wxStaticText" name="existing_st">										<enabled>0</enabled>										<label>Open existing project folder</label>										<wrap>-1</wrap>									</object>								</object>								<object class="sizeritem">									<option>0</option>									<flag>wxALL|wxEXPAND</flag>									<border>5</border>									<object class="wxDirPickerCtrl" name="existing_dp">										<value></value>										<message>Select a folder</message>										<style>wxDIRP_DEFAULT_STYLE</style>										<enabled>0</enabled>									</object>								</object>								<object class="sizeritem">									<option>0</option>									<flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>									<border>5</border>									<object class="wxButton" name="install_b">										<enabled>0</enabled>										<label>Install and initialize project</label>										<default>0</default>									</object>								</object>							</object>						</object>					</object>					<object class="notebookpage">						<label>Build ROM</label>						<selected>0</selected>						<object class="wxPanel" name="build_page">							<style>wxTAB_TRAVERSAL</style>							<enabled>0</enabled>							<object class="wxBoxSizer">								<orient>wxVERTICAL</orient>								<object class="spacer">									<option>1</option>									<flag>wxEXPAND</flag>									<border>5</border>									<size>0,0</size>								</object>								<object class="sizeritem">									<option>0</option>									<flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>									<border>5</border>									<object class="wxButton" name="build_b">										<label>Build ROM</label>										<default>0</default>									</object>								</object>								<object class="spacer">									<option>1</option>									<flag>wxEXPAND</flag>									<border>5</border>									<size>0,0</size>								</object>							</object>						</object>					</object>					<object class="notebookpage">						<label>Flash ROM</label>						<selected>0</selected>						<object class="wxPanel" name="flash_page">							<style>wxTAB_TRAVERSAL</style>							<object class="wxBoxSizer">								<orient>wxVERTICAL</orient>								<object class="spacer">									<option>1</option>									<flag>wxEXPAND</flag>									<border>5</border>									<size>0,0</size>								</object>								<object class="sizeritem">									<option>0</option>									<flag>wxALIGN_CENTER_HORIZONTAL|wxALL</flag>									<border>5</border>									<object class="wxButton" name="flash_b">										<label>Flash ROM</label>										<default>0</default>									</object>								</object>								<object class="spacer">									<option>1</option>									<flag>wxEXPAND</flag>									<border>5</border>									<size>0,0</size>								</object>							</object>						</object>					</object>				</object>			</object>			<object class="sizeritem">				<option>0</option>				<flag>wxEXPAND</flag>				<border>5</border>				<object class="wxBoxSizer">					<orient>wxVERTICAL</orient>					<object class="sizeritem">						<option>0</option>						<flag>wxEXPAND | wxALL</flag>						<border>5</border>						<object class="wxStaticLine" name="m_staticline1">							<style>wxLI_HORIZONTAL</style>						</object>					</object>					<object class="sizeritem">						<option>0</option>						<flag>wxALL|wxEXPAND</flag>						<border>5</border>						<object class="wxGauge" name="progress">							<style>wxGA_HORIZONTAL</style>							<range>100</range>							<value>0</value>						</object>					</object>					<object class="sizeritem">						<option>1</option>						<flag>wxEXPAND</flag>						<border>5</border>						<object class="wxBoxSizer">							<orient>wxHORIZONTAL</orient>							<object class="sizeritem">								<option>0</option>								<flag>wxALL</flag>								<border>5</border>								<object class="wxStaticText" name="m_staticText21">									<label>Output</label>									<wrap>-1</wrap>								</object>							</object>							<object class="spacer">								<option>1</option>								<flag>wxEXPAND</flag>								<border>5</border>								<size>0,0</size>							</object>							<object class="sizeritem">								<option>0</option>								<flag>wxALL</flag>								<border>5</border>								<object class="wxButton" name="clear_b">									<label>Clear</label>									<default>0</default>								</object>							</object>						</object>					</object>					<object class="sizeritem">						<option>0</option>						<flag>wxALL|wxEXPAND</flag>						<border>5</border>						<object class="wxTextCtrl" name="details_tc">							<style>wxHSCROLL|wxTE_MULTILINE|wxTE_READONLY</style>							<size>-1,150</size>							<value></value>						</object>					</object>				</object>			</object>		</object>	</object></resource>